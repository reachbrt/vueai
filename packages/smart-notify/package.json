{
  "name": "@aivue/smart-notify",
  "version": "1.0.0",
  "description": "AI-powered smart notification system for Vue 3 - Learns optimal timing, groups intelligently, detects urgency with NLP, and respects user attention",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./style.css": "./dist/smart-notify.css",
    "./components/*": {
      "import": "./dist/components/*",
      "require": "./dist/components/*"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run clean && vite build && npm run postbuild",
    "dev": "vite --watch",
    "lint": "eslint \"src/**/*.{ts,vue}\"",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "postbuild": "cp -r src/components dist/components && cp -r src/composables dist/composables && cp -r src/utils dist/utils && cp -r src/ai dist/ai && cp README.md dist/ && cp CHANGELOG.md dist/ 2>/dev/null || true"
  },
  "keywords": [
    "vue",
    "ai",
    "notifications",
    "smart-notifications",
    "notification-manager",
    "machine-learning",
    "nlp",
    "urgency-detection",
    "attention-detection",
    "notification-batching",
    "intelligent-grouping",
    "optimal-timing",
    "context-aware",
    "offline-ml",
    "privacy-focused",
    "ui",
    "components"
  ],
  "author": "reachbrt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/reachbrt/vueai.git",
    "directory": "packages/smart-notify"
  },
  "homepage": "https://github.com/reachbrt/vueai#readme",
  "bugs": {
    "url": "https://github.com/reachbrt/vueai/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "compromise": "^14.10.0"
  },
  "peerDependencies": {
    "@aivue/core": "^1.3.2",
    "vue": "^2.6.0 || ^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.16.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^8.56.0",
    "jsdom": "^27.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.0.0",
    "vue": "^3.5.0"
  }
}
