{
  "name": "@aivue/image-caption",
  "version": "1.0.0",
  "description": "AI-powered image captioning for Vue.js applications using Hugging Face BLIP models",
  "author": "reachbrt",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/image-caption.css": "./dist/image-caption.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.vue",
    "typecheck": "vue-tsc --noEmit"
  },
  "keywords": [
    "vue",
    "vue3",
    "ai",
    "image-captioning",
    "computer-vision",
    "huggingface",
    "blip",
    "machine-learning",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reachbrt/vueai.git",
    "directory": "packages/image-caption"
  },
  "homepage": "https://github.com/reachbrt/vueai/tree/main/packages/image-caption",
  "bugs": {
    "url": "https://github.com/reachbrt/vueai/issues"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@aivue/core": "^1.2.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
