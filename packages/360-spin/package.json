    "access": "public"
  },
  "peerDependencies": {
    "vue": "^2.6.0 || ^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.16.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/compiler-sfc": "^3.5.13",
